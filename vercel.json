{
    "cleanUrls": true,
    "trailingSlash": true,
    "rewrites": [
        {
            "source": "/tech",
            "destination": "/sites/tech/index.html"
        },
        {
            "source": "/tech/",
            "destination": "/sites/tech/index.html"
        },
        {
            "source": "/tech/:path*",
            "destination": "/sites/tech/:path*"
        },
        {
            "source": "/",
            "destination": "/sites/commercial/index.html"
        },
        {
            "source": "/:path*",
            "destination": "/sites/commercial/:path*"
        }
    ]
}